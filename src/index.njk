                </div>
            </div>
        {% endif %}
        {# Map #}
        {% if config.show_map == 'true' %}
            {% include "components/map.njk" ignore missing %}
            <script src="js/geo.js"></script>
        {% endif %}

        {# Footer #}
        <div class="container section section_footer">
            <footer class="footer">
                {% include "components/footer.njk" ignore missing %}
            </footer>
        </div>
    </body>
</html>
